{
  "name": "discord-app-bot",
  "version": "0.1.0",
  "description": "A Discord bot for automating the handling of Google Forms pushed to a server.",
  "repository": "github:adamhl8/discord-app-bot",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint -f pretty --fix .",
    "restart": "tsc && pm2 restart discord-app-bot",
    "start": "tsc && pm2 start ecosystem.config.yaml"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "node-persist": "^3.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/node": "^16.4.2",
    "@types/node-persist": "^3.1.2",
    "@types/prettier": "^2.3.2",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-unicorn": "^34.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "prettier-plugin-pkg": "^0.10.0",
    "prettier-plugin-sh": "^0.7.1",
    "typescript": "^4.3.5"
  }
}
